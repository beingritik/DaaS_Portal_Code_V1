<!Doctype html>

<head>
    <%- include('../common/cdn') %>
        <title>Assign User Page</title>
        <link rel="stylesheet" href="/css/sidebar.css">
        <link rel="stylesheet" href="/css/logs.css">
        <link rel="stylesheet" href="/css/style.css">
        <link rel="stylesheet" href="/css/footer.css">
</head>

<body class="body_background_color">
    <%- include('../common/header') %>

    <div class="container">
        <div class="row">
            <div class="col-lg-6 ">
                <div class="logs">
                        <h5 class="logs_heading">Assigned User Logs displayed here</h5>
                        <%if(revoke_vdi){%>
                            <% for(i=0;i<revoke_vdi.length;i++){ %>
                                <span class="logs_color">   <%= i+1%>  . The <%= manage_vdi_access_assign_logs[i].assigned_user%> is assigned to 
                                    <%= manage_vdi_access_assign_logs[i].assigned_to_userGroup %>- by 
                                    <%=manage_vdi_access_assign_logs[i].assigned_by_user %> at 
                                    <%=manage_vdi_access_assign_logs[i].timestamp%>  . 
                                </span>
                                <br>
                            <%}%>
                        <%}%>
                    </div>
                </div>
            
            <div class="col-lg-6 ">
                <div class="logs">
                    <h5 class="logs_heading"> The logs for deletion are : </h5>
                    <%if(revoke_vdi){%>
                        <% for(i=0;i<revoke_vdi.length;i++){ %>
                            <span class="delete_logs_color">   <%= i+1%>  . The <%=revoke_vdi[i].deleted_user%> deleted by
                            <%=revoke_vdi[i].perform_delete_by_user%> to the
                            <%=revoke_vdi[i].deleted_from_userGroup%> at
                            <%=revoke_vdi[i].date_time_of_delete%> .
                            </span>
                            <br>
                        <%}%>
                    <%}%>
                </div>
            </div>
            
        </div>
        <div class="row">
            <div calss="col-lg-6 col-sm-6">
                <div class="logs">
                    <h5 class="logs_heading"> The Admin Logs are : </h5>
                    <% if(admin_portal_logs){ %>
                        <% for(i=0;i<admin_portal_logs.length;i++){ %>
                            <span class="admin_logs_color">   <%= i+1%>  . The <%=admin_portal_logs[i].user%> <%= admin_portal_logs[i].action %> by
                            <%=admin_portal_logs[i].by_user%> from/in the
                            <%=admin_portal_logs[i].userGroup%> at
                            <%=admin_portal_logs[i].date_time%> .
                            </span>
                            <br>
                        <%}%>
                    <%}%>

                </div>
            </div>
        </div>
        <script src="/js/userGroup.js"></script>

        <%- include('../common/fscript') %>
            <!-- html closed here -->
            <%- include('../common/footer') %>